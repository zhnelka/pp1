//W. Тип треугольника
#include <iostream>
#include <algorithm>

using namespace std;

int main() {
    int a, b, c;
    cin >> a >> b >> c;

    if (a + b <= c || a + c <= b || b + c <= a) {
        cout << "impossible" << endl;
    } else {
        int max_side = max({a, b, c});
        int sum_of_squares = a * a + b * b + c * c - max_side * max_side;

        if (sum_of_squares == max_side * max_side) {
            cout << "right" << endl;
        } else if (sum_of_squares > max_side * max_side) {
            cout << "acute" << endl;
        } else {
            cout << "obtuse" << endl;
        }
    }

    return 0;
}
